        <view bindtap="__e" class="birthday-btn f-semibold data-v-e64b2a78" data-event-opts="{{[ [ 'tap',[ [ 'confirmBirthday',['$event'] ] ] ] ]}}">好的</view>
        <image catchtap="__e" class="model-box-close data-v-e64b2a78" data-event-opts="{{[ [ 'tap',[ [ 'clickCancelBithday',['$event'] ] ] ] ]}}" src="{{assetsImage.birthdayClose}}"></image>
