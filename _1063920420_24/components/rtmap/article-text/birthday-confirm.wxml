<view catchtap="__e" class="v-delete-model data-v-e64b2a78" data-event-opts="{{[ [ 'tap',[ [ 'clickCancelMask',['$event'] ] ] ] ]}}" wx:if="{{showDeleteModel}}">
    <view class="model-box data-v-e64b2a78" wx:if="{{!isBirthday}}">
        <view class="model-box-top data-v-e64b2a78">
            <om-image bind:__l="__l" class="data-v-e64b2a78" end="?imageMogr2/format/png/interlace/1" height="{{100}}" mode="aspectFill" url="{{assetsImage.editBirthdayBg}}" vueId="5527994b-1" width="{{275}}"></om-image>
        </view>
        <view class="model-box-bottom data-v-e64b2a78">
            <view class="title f-semibold _p data-v-e64b2a78">
                <rich-text class="data-v-e64b2a78" nodes="{{$root.g0}}"></rich-text>
            </view>
            <view class="action data-v-e64b2a78">
                <view catchtap="__e" class="btn sure f-semibold data-v-e64b2a78" data-event-opts="{{[ [ 'tap',[ [ 'clickConfirm',['$event'] ] ] ] ]}}">确定</view>
